<configuration>
  <!--
    For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.5" />
      </system.Web>
  -->
  <system.web>
    <sessionState mode="StateServer" timeout="525600"/>
    <customErrors mode="Off"/>
    <compilation targetFramework="4.5"/>
    <pages controlRenderingCompatibilityVersion="3.5" clientIDMode="AutoID"/>
  </system.web>
  <connectionStrings>
    <add name="KioskConnectionString" connectionString="Data Source=SSServer,1433;Network Library=DBMSSOCN;Initial Catalog=kiosk;User ID=yourID;Password=yourPassword"/>
  </connectionStrings>
</configuration>